{
  "name": "agent-command-sync",
  "version": "5.4.0",
  "description": "A skill package manager for AI coding agents â€” download, update, and sync Skills across Claude Code, Gemini CLI, Codex CLI, OpenCode, GitHub Copilot, and Cursor. Zero config, no extra files needed.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-command-sync": "./bin/agent-command-sync",
    "acs": "./bin/agent-command-sync"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "lint:tsc": "tsc --noEmit",
    "format": "biome format --write .",
    "check": "npm run lint && npm run lint:tsc && npm run test",
    "prepublishOnly": "npm run check && npm run build",
    "preversion": "npm run check",
    "version": "npm run format && git add -A",
    "npm:login": "npm login",
    "npm:whoami": "npm whoami"
  },
  "files": ["dist", "bin"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": ["claude-code", "gemini-cli", "slash-commands", "converter", "cli"],
  "author": "hatappo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hatappo/agent-command-sync.git"
  },
  "bugs": {
    "url": "https://github.com/hatappo/agent-command-sync/issues"
  },
  "homepage": "https://github.com/hatappo/agent-command-sync#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@iarna/toml": "2.2.5",
    "commander": "14.0.0",
    "gray-matter": "4.0.3",
    "picocolors": "1.1.1",
    "yaml": "2.8.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "24.2.0",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "5.9.2",
    "vite": "7.0.6",
    "vitest": "3.2.4"
  },
  "volta": {
    "node": "24.5.0",
    "npm": "11.5.2"
  }
}
